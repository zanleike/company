page {
  background-color: #F8F8F8;
  width: 100%;
  height: 100%;
  font-size: 32rpx;
  line-height: 1.6;
  padding: 0;
  margin:0;
}

.container{
  box-sizing: border-box;
  width: 100%;
  padding-top: 0rpx;
  height: 100%;
}

.page-body{
  box-sizing: border-box;
  padding-top: 0rpx;
  height: 100%;
  width: 100%;
}

.page-section-spacing{
  height: 240px;
  width: 100%; 
  margin-bottom: 0rpx;
  border-bottom: 10rpx solid rgb(240, 239, 239);
  box-sizing: border-box;
  padding: 0 0rpx;
}

.image1{
  display: block;
  margin:0px auto;
}
.image2{
  display: block;
  margin:0px auto;
}
.page-section-button{
  display: flex;
  flex-direction: row;
  /* flex-wrap: wrap; */
  width: 100%;
  /* margin: 0,auto; */
  /* position: relative; */
  border-bottom: 10rpx solid rgb(240, 239, 239);
  /* left: 5px; */
}

.link_child {
  width: 33%;
  height: 90px;
  background: white;
  /* border: 1rpx solid rgb(233, 231, 231); */
  /* flex-direction:column; */
 } 
 /* .link_child:not(:nth-child(3n)){
  margin-right: 10rpx; 
 } */
 .link_view{
  display: block;
  overflow: hidden;
  width: 100%;
  height: 80%;
  border: 0rpx solid rgb(233, 231, 231); 
  position: relative;
 }
 .link_image{
  display: block;
  width: 50%;
  height: 60%;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  border: 0rpx solid rgb(233, 231, 231); 
 }

 .section__title{
  display: block;
  width: 100%;
  height: 20%;
  text-align: center;
  color: #036b66;
  font-size: 11px;
  font-style: normal;
  font-weight: bold;
 }
  

